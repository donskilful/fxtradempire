<!DOCTYPE html>
<html>

<head>
	<!-- <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1"> -->
    <!-- Favicon -->
    <link rel="shortcut icon" href="web/images/favicon.png">
	<title>Admin | Crypto-Exchangerz</title>
	<link href="css/bootstrap.min.css" rel="stylesheet">
	<link href="css/font-awesome.min.css" rel="stylesheet">
	<link href="css/datepicker3.css" rel="stylesheet">
	<link href="css/styles.css" rel="stylesheet">

	<!--Custom Font-->
	<link href="https://fonts.googleapis.com/css?family=Montserrat:300,300i,400,400i,500,500i,600,600i,700,700i"
		rel="stylesheet">
	<!--[if lt IE 9]>
	<script src="js/html5shiv.js"></script>
	<script src="js/respond.min.js"></script>
	<![endif]-->
</head>

<body>
	<nav class="navbar navbar-custom navbar-fixed-top" role="navigation">
		<div class="container-fluid">
			<div class="navbar-header">
				<button type="button" class="navbar-toggle collapsed" data-toggle="collapse"
					data-target="#sidebar-collapse"><span class="sr-only">Toggle navigation</span>
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
					<span class="icon-bar"></span></button>
				<a class="navbar-brand" href="#"><span>Crypto-</span><strong>Exchangerz</strong></a>
				<div class="user-icon">
					<i class="fa fa-user-circle-o fa-2x user" aria-hidden="true"></i>
				</div>
			</div>
		</div><!-- /.container-fluid -->
	</nav>
	<div id="sidebar-collapse" class="col-sm-3 col-lg-2 sidebar">
		<div class="profile-sidebar">
			<div class="profile-userpic">
				<img src="http://placehold.it/50/30a5ff/fff" class="img-responsive" alt="">
			</div>
			<div class="profile-usertitle">
				<div class="profile-usertitle-name">Admin</div>
				<div class="profile-usertitle-status"><span class="indicator label-success"></span>Online</div>
			</div>
			<div class="clear"></div>
		</div>
		<div class="divider"></div>
		<ul class="nav menu">
			<li><a href="/tradecenter"><em class="fa fa-bar-chart">&nbsp;</em> Trade Center</a></li>
			<li><a href="/profile"><em class="fa fa-user">&nbsp;</em> Profile</a></li>
			<li><a href="/deposit"><em class="fa fa-btc">&nbsp;</em> Deposit</a></li>
			<li><a href="/withdrawal"><em class="fa fa-money">&nbsp;</em> Withdrawal</a></li>
			<li><a href="/upgradeaccount"><em class="fa fa-arrow-up">&nbsp;</em> Upgrade Account</a></li>
			<li><a href="/admin"><em class="fa fa-user-circle-o">&nbsp;</em> Admin</a></li>
			<li><a href="/login"><em class="fa fa-sign-out">&nbsp;</em> Logout</a></li>
		</ul>
	</div>
	<!--/.sidebar-->

	<div class="col-sm-9 col-sm-offset-3 col-lg-10 col-lg-offset-2 main">
		<div class="row">
			<ol class="breadcrumb">
				<li><a href="#">
						<em class="fa fa-home"></em>
					</a></li>
				<li class="active">ADMIN</li>
			</ol>
		</div>
		<!--/.row-->

		<div class="row header-title">
			<div class="col-lg-12">
				<h3 class="page-header">Admin</h3>
			</div>
		</div>
        <% users.forEach(function(users){  %>
		<div>
			<table class="table" style="width: 90%; margin: 30px;">
				<tr>
					<th>FullName</th>
					<th>Phone</th>
					<th>Country</th>
					<th>Email</th>
					<th>Progress</th>
					<th>Reg. Date</th>
					<th>Balance</th>
					<th>Action</th>
					
				</tr>
		
				<tr>
					<td><%= users.firstname %> <%= users.lastname %></td>
					<td><%= users.phone %></td>
					<td><%= users.country %></td>
					<td><%= users.email %></td>
					<td>% <%= users.progress %></td>
					<td><%=users.date.toDateString()%></td>
					<td>$ <%= users.balance %></td>
					
					<td><a href="/admin%_/<%= users._id %>/edit">Edit</a></td>
				</tr>
			</table>
		</div>
        <% }); %>

	</div>
	<!--/.main-->

	<script src="js/jquery-1.11.1.min.js"></script>
	<script src="js/bootstrap.min.js"></script>
	<script src="js/chart.min.js"></script>
	<script src="js/chart-data.js"></script>
	<script src="js/easypiechart.js"></script>
	<script src="js/easypiechart-data.js"></script>
	<script src="js/bootstrap-datepicker.js"></script>
	<script src="js/custom.js"></script>
	<script>
		window.onload = function () {
			var chart1 = document.getElementById("line-chart").getContext("2d");
			window.myLine = new Chart(chart1).Line(lineChartData, {
				responsive: true,
				scaleLineColor: "rgba(0,0,0,.2)",
				scaleGridLineColor: "rgba(0,0,0,.05)",
				scaleFontColor: "#c5c7cc"
			});
		};
	</script>

</body>

</html>


